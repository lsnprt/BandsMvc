@model Band[];
@using BandsMvc.Models;

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/skeleton/2.0.4/skeleton.min.css">
    <title>Band</title>
</head>
<body>
<div>
    <ul>
        @foreach (var band in Model)
            {
                string link = $"/bands/details/{band.Id}";
                <li>
                    <a href=@link>@band.Name</a>
                </li>
            }
    </ul>
</div>
</body>
</html>